'title': 'View cannot be created if its WHERE clause contains IN <subquery> with a procedure reference',
'description': '',
'versions': [
{
 'firebird_version': '2.0.2',
 'platform': 'All',
 'init_script': """set term ^;

create procedure p
  returns (col int)
as
begin
  col = 1;
  suspend;
end^

set term ;^

commit;""",
 'test_type': 'ISQL',
 'test_script': """create view v
